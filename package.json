{
    "name": "users-vue",
    "version": "0.0.1",
    "description": "A base app with users",
    "productName": "Users vue",
    "author": "Marios Tsigkas <mariosh346@gmail.com>",
    "private": true,
    "type": "module",
    "scripts": {
        "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
        "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
        "dev": "quasar dev",
        "build": "quasar build",
        "serve": "quasar serve dist",
        "postinstall": "quasar prepare",
        "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://127.0.0.1:8080 \"cypress open --e2e\"",
        "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://127.0.0.1:8080 \"cypress run --e2e\"",
        "test:component": "cross-env NODE_ENV=test cypress open --component",
        "test:component:ci": "cross-env NODE_ENV=test cypress run --component",
        "test:unit": "vitest",
        "test:unit:ci": "vitest run",
        "lighthouse": "node test/lightHouse.js",
        "setAdmin": "npx tsx scripts/setAdmin.ts USER_ID_TO_SET_ADMIN",
        "deploy": "gh-pages -d dist"
    },
    "dependencies": {
        "@pinia/testing": "^0",
        "@quasar/extras": "^1.16.4",
        "axios": "^1.8.2",
        "chrome-launcher": "^1.1.2",
        "firebase": "^11.2.0",
        "firebase-admin": "^13.1.0",
        "lighthouse": "^12.3.0",
        "pinia": "^2.3.1",
        "quasar": "^2.17.7",
        "ts-node": "^10.9.2",
        "vue": "^3.4.18",
        "vue-i18n": "^9.14.3",
        "vue-router": "^4.0.12",
        "vuefire": "^3.2.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.14.0",
        "@intlify/unplugin-vue-i18n": "^2.0.0",
        "@quasar/app-vite": "^2.2.0",
        "@quasar/quasar-app-extension-testing-e2e-cypress": "^6.1.0",
        "@quasar/quasar-app-extension-testing-unit-vitest": "^1.1.0",
        "@types/node": "^20.17.17",
        "@vue/eslint-config-typescript": "^14.1.3",
        "@vue/test-utils": "^2.4.4",
        "autoprefixer": "^10.4.2",
        "cypress": "^13.6.6",
        "eslint": "^9.14.0",
        "eslint-plugin-cypress": "^2.15.1",
        "eslint-plugin-vue": "^9.30.0",
        "globals": "^15.12.0",
        "tsx": "^4.19.3",
        "typescript": "~5.5.3",
        "vite-plugin-checker": "^0.9.1",
        "vitest": "^2.1.9",
        "vue-tsc": "~2.2.2"
    },
    "engines": {
        "node": "^22",
        "pnpm": ">= 6.13.4"
    }
}